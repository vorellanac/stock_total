

<h1>Productos</h1>
<!-- show  -->
<div class="container">
<table class="table" width="20%">
  <thead>
    <tr>
      <th>SKU</th>
      <th>Descripcion</th>
      <th>Precio</th>
      <th>imagen</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<!-- end show list item products -->

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.sku %></td>
        <td><%= product.name %></td>
        <td><%= product.price %></td>
        <td><%= product.picture %></td>
        <td><%= link_to 'Ver detalle', product %></td>
  
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>